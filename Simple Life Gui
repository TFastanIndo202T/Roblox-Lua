HitboxSetButton.Name = "Hitbox Set Button"
HitboxSetButton.Parent = Page1Frame
HitboxSetButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
HitboxSetButton.BorderColor3 = Color3.fromRGB(72, 72, 72)
HitboxSetButton.BorderSizePixel = 3
HitboxSetButton.Position = UDim2.new(0.594733, 0, 0.214559376, 0)
HitboxSetButton.Size = UDim2.new(0, 78, 0, 22)
HitboxSetButton.Font = Enum.Font.Code
HitboxSetButton.Text = "Set Hitbox"
HitboxSetButton.TextColor3 = Color3.fromRGB(44, 44, 44)
HitboxSetButton.TextSize = 14.000
HitboxSetButton.MouseButton1Click:connect(function()
	for i, v in pairs(game.Players:GetPlayers()) do
		if v.Name ~= game.Players.LocalPlayer.Name then
			pcall(function()
				v.Character.HumanoidRootPart.Size = Vector3.new(Hitboxcustom.Text, Hitboxcustom.Text, Hitboxcustom.Text)
				v.Character.HumanoidRootPart.Transparency = 0.8
				v.Character.HumanoidRootPart.BrickColor = BrickColor.new("Light grey")
				v.Character.HumanoidRootPart.Material = "SmoothPlastic"
				v.Character.HumanoidRootPart.CanCollide = false


				Rainbow.MouseButton1Click:Connect(function()
					local TweenService = game:GetService("TweenService")
					local part = v.Character.HumanoidRootPart
					local TweeningInformation = TweenInfo.new(
						2,
						Enum.EasingStyle.Quad,
						Enum.EasingDirection.Out,
						753475938457843579348573,
						true,
						0
					)

					local PartProperties = {
						Color = Color3.fromRGB(255,0,0)
					}

					local Tween = TweenService:Create(part,TweeningInformation,PartProperties)
					Tween:Play()
					Rainbow.BackgroundColor3 = Color3.fromRGB(0,225,0)


				end)
			end)
		end
	end
end)
ConfirmButton.MouseButton1Click:connect(function()
	if Key.Text == "No Key LOL" then
		MainFrame.Visible = true
		LoginFrame.Visible = false
	end
end)

